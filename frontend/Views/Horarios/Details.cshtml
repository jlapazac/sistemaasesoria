@model frontend.Dominio.Horario
@{
    ViewBag.Title = "Details";
}

<h2>@ViewData["Message"]</h2>

<div>
    <h4>Alumno</h4>
    <hr/>
    <dl class="dl-horizontal">
        <dt>
            @ViewBag.Titulo01
        </dt>
        <dd>
            @Html.DisplayFor(model => model.codhorario)
        </dd>
        <dt>
            @ViewBag.Titulo02
        </dt>
        <dd>
            @Html.DisplayFor(model => model.codciclo)
        </dd>
        <dt>
