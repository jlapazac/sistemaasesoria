        <dt>
            @ViewBag.Titulo05
        </dt>
        <dd>
            @Html.DisplayFor(model => model.finicio)
        </dd>
        <dt>
            @ViewBag.Titulo06
        </dt>
        <dd>
            @Html.DisplayFor(model => model.ffinal)
        </dd>
        <dt>
            @ViewBag.Titulo07
        </dt>
        <dd>
            @Html.DisplayFor(model => model.estado)
        </dd>
